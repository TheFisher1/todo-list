{
  "name": "task-service",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "knex migrate:latest --knexfile src/db/knexfile.js --env development",
    "migrate:test": "knex migrate:latest --knexfile src/db/knexfile.js --env test",
    "migrate:rollback": "knex migrate:rollback --knexfile src/db/knexfile.js --env development",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "knex": "^3.1.0",
    "mongoose": "^6.0.12",
    "objection": "^3.1.4",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "nodemon": "^2.0.14"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "extensionsToTreatAsEsm": [
      ".js"
    ],
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
  }
}
